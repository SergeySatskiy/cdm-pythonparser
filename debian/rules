#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.8.5 at
# Mon, 01 Feb 2016 00:04:37 -0500

%:
	dh $@ --with python2 --buildsystem=python_distutils


override_dh_auto_clean:
	python setup.py clean -a
	find . -name \*.pyc -exec rm {} \;



override_dh_auto_build:
	CDM_PYPARSER_BUILD_VERSION=$(CDM_PYPARSER_BUILD_VERSION) python setup.py build --force



override_dh_auto_install:
	CDM_PYPARSER_BUILD_VERSION=$(CDM_PYPARSER_BUILD_VERSION) python setup.py install --force --root=debian/python-cdmpyparser --no-compile -O0 --install-layout=deb  



override_dh_python2:
	dh_python2 --no-guessing-versions




