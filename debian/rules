#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.8.5 at
# Mon, 01 Feb 2016 00:04:37 -0500

DEBVERS := $(shell dpkg-parsechangelog | sed -n -e 's/^Version: //p')
CDM_PROJECT_BUILD_VERSION := $(shell echo '$(DEBVERS)' | sed -e 's/^[[:digit:]]*://' -e 's/[~-].*//')

%:
	dh $@ --with python2 --buildsystem=python_distutils


override_dh_auto_clean:
	CDM_PROJECT_BUILD_VERSION=$(CDM_PROJECT_BUILD_VERSION) python setup.py clean -a
	find . -name \*.pyc -exec rm {} \;



override_dh_auto_build:
	CDM_PROJECT_BUILD_VERSION=$(CDM_PROJECT_BUILD_VERSION) python setup.py build --force



override_dh_auto_install:
	CDM_PROJECT_BUILD_VERSION=$(CDM_PROJECT_BUILD_VERSION) python setup.py install --force --root=debian/cdm-pythonparser --no-compile -O0 --install-layout=deb  



override_dh_python2:
	dh_python2 --no-guessing-versions




